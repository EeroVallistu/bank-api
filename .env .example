# -- SETTINGS YOU SHOULD CHANGE --
JWT_SECRET=change_this_to_a_secure_random_string         # Generate a strong random string
BANK_NAME=Your Bank Name                                # Your bank's name
BANK_OWNERS=Your Name or Organization                   # Bank's owner(s)
TRANSACTION_URL=https://your-domain.com/transactions/b2b # Your bank's transaction endpoint
JWKS_URL=https://your-domain.com/jwks.json              # Your bank's JWKS endpoint

# -- SETTINGS PROVIDED BY CENTRAL BANK DURING REGISTRATION --
API_KEY=your_central_bank_api_key                       # Received from central bank during registration

# -- DEFAULT SETTINGS (can leave unchanged) --
PORT=9000
CENTRAL_BANK_URL=https://henno.cfd/central-bank
NODE_ENV=development
DISABLE_SEQUELIZE_LOGGING=true

# -- DATABASE CONNECTION SETTINGS --
USE_DATABASE=true
DB_HOST=localhost
DB_PORT=3306
DB_NAME=bank_api
DB_USER=root
DB_PASSWORD=password
DB_DIALECT=mysql